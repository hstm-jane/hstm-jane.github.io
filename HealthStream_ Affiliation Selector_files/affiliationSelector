(function(n,t){"use strict";n.HealthStream=n.HealthStream||{};var i=n.HealthStream;n.HealthStream.login=n.HealthStream.login||{},i.login.initialize=function(){t(".nav-tab").on("click",function(n){var i=t(this).attr("href");t(".nav-tab, .tab-pane").removeClass("selected"),t(".has-error").removeClass("has-error"),t(".alert").removeClass("active"),t(this).addClass("selected"),t(i).addClass("selected").scrollTop(0),n.preventDefault()});t(".slat-affiliate").on("click",function(){t(".slat-affiliate.selected").removeClass("selected"),t('.slat-affiliate input[type="radio"]:checked').closest(".slat-affiliate").addClass("selected")});t(".pw-reminder").on("click",function(n){t(".alert").removeClass("active"),t("#alert-reminder").addClass("active"),n.preventDefault()});t(".pw-reset").on("click",function(n){t("#tab-password-reset .form-control").val(""),t(".tab-pane").removeClass("selected"),t("#tab-password-reset").addClass("selected"),n.preventDefault()});t("#button-reset").click(function(n){t(".alert").removeClass("active");var i="";t(this).closest("form").find(".required").each(function(){i+=t(this).val()}),i===""?t(this).closest("form").addClass("has-error"):(t(this).closest("form").removeClass("has-error"),t('.nav-tab[href="#tab-signin"]').click(),t("#alert-reset").addClass("active")),n.preventDefault()}),t("#button-login").click(function(n){var i="";t(".alert").removeClass("active"),t(this).closest("form").find(".required").each(function(){i+=t(this).val(),t(this).val()===""?t(this).closest(".form-group").addClass("has-error"):t(this).closest(".form-group").removeClass("has-error")}),i!==""&&t("#alert-unauthorized").addClass("active"),n.preventDefault()}),t("#button-update").click(function(n){var i="";t(".alert").removeClass("active"),t(this).closest("form").find(".required").each(function(){i+=t(this).val(),t(this).val()===""?(t(this).closest(".form-group").addClass("has-error"),n.preventDefault()):t(this).closest(".form-group").removeClass("has-error")})})};t(n).on("load",function(){i.login.initialize()})})(window,jQuery)